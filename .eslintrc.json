{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "rules": {
        // "dot-location": [
        //     "warn",
        //     "property"
        // ],
        // "quotes": [
        //     "warn",
        //     "single"
        // ],
        // "semi": [
        //     "warn",
        //     "always"
        // ],
        // "comma-dangle": [
        //     "error",
        //     "never"
        // ]
    }
}