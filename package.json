{
    "name": "react-starter",
    "version": "1.0.0",
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mebinrobin/react-starter.git"
    },
    "bugs": {
        "url": "https://github.com/mebinrobin/react-starter/issues"
    },
    "homepage": "https://github.com/mebinrobin/react-starter#readme",
    "description": "",
    "scripts": {
        "build": "./node_modules/.bin/webpack --hide-modules --env.production",
        "dev": "./node_modules/.bin/webpack-dev-server --env.development",
        "make-config-only-branch": "git checkout config-only && "
    },
    "author": {
        "name": "Mebin Robin",
        "url": "https://www.mebinrobin.com"
    },
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "^7.5.5",
        "@babel/core": "^7.5.5",
        "@babel/preset-env": "^7.5.5",
        "@babel/preset-react": "^7.0.0",
        "babel-eslint": "^10.0.3",
        "babel-loader": "^8.0.6",
        "clean-webpack-plugin": "^3.0.0",
        "css-loader": "^3.2.0",
        "eslint": "^6.3.0",
        "eslint-plugin-react": "^7.14.3",
        "html-webpack-plugin": "^3.2.0",
        "mini-css-extract-plugin": "^0.8.0",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "postcss-loader": "^3.0.0",
        "postcss-preset-env": "^6.7.0",
        "script-ext-html-webpack-plugin": "^2.1.4",
        "style-loader": "^1.0.0",
        "terser-webpack-plugin": "^1.4.1",
        "webpack": "^4.39.3",
        "webpack-cli": "^3.3.7",
        "webpack-dev-server": "^3.8.0"
    },
    "dependencies": {
        "normalize.css": "^8.0.1",
        "react": "^16.9.0",
        "react-dom": "^16.9.0"
    },
    "browserslist": [
        ">0.1%",
        "not dead"
    ],
    "postcss": {
        "map": false
    },
    "eslintConfig": {
        "parser": "babel-eslint",
        "env": {
            "browser": true,
            "node": true
        },
        "parserOptions": {
            "ecmaVersion": 8,
            "sourceType": "module",
            "ecmaFeatures": {
                "jsx": true
            }
        },
        "extends": [
            "eslint:recommended",
            "plugin:react/recommended"
        ],
        "settings": {
            "react": {
                "version": "detect"
            }
        },
        "rules": {
            "dot-location": [
                "warn",
                "property"
            ],
            "quotes": [
                "warn",
                "single"
            ],
            "semi": [
                "warn",
                "always"
            ],
            "comma-dangle": [
                "error",
                "never"
            ]
        }
    }
}
