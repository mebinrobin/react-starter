{
  "name": "react-starter",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mebinrobin/react-starter.git"
  },
  "bugs": {
    "url": "https://github.com/mebinrobin/react-starter/issues"
  },
  "homepage": "https://github.com/mebinrobin/react-starter#readme",
  "description": "",
  "scripts": {
    "build": "webpack --hide-modules --env.production",
    "dev": "webpack-dev-server --env.development",
    "make-config-only-branch": "git checkout config-only && git checkout master webpack.config.js && git checkout master package.json && git add webpack.config.js && git add package.json && git commit -m 'Update config' && git push"
  },
  "author": {
    "name": "Mebin Robin",
    "url": "https://www.mebinrobin.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "eslint": "^6.3.0",
    "eslint-plugin-react": "^7.14.3",
    "html-webpack-plugin": "^4.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "sass-loader": "^8.0.0",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "terser-webpack-plugin": "^2.0.1",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0"
  },
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "browserslist": [
    ">0.1%",
    "not dead"
  ],
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      "dot-location": [
        "warn",
        "property"
      ],
      "quotes": [
        "warn",
        "single"
      ],
      "semi": [
        "warn",
        "always"
      ],
      "comma-dangle": [
        "error",
        "never"
      ]
    }
  }
}
